    "react-build": "react-scripts build",
    "react-serve": "SET BROWSER=none&&yarn react-start",
    "lint": "eslint src/**/*.{ts,tsx}",
    "commit": "git-cz"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "homepage": "./",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.19.8",
